{
  "compilerOptions": {
    "target": "ES5",
    "module": "commonjs",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": false,
    "outDir": "app/js"
  },
  "filesGlob": [
    "./app/ts/*.ts"
  ],
  "files": [
    "./app/ts/app.ts",
    "./app/ts/category.ts",
    "./app/ts/company.ts",
    "./app/ts/funds.ts",
    "./app/ts/model.ts",
    "./app/ts/mutualFundService.ts"
  ],
  "exclude": [],
  "compileOnSave": true
}
